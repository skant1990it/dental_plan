
<form name="userForm" id="msform">
    <header>
        <div class="logo"><a href="javascript:void(0);" title="denefits"><img src="images/logo.png" width="183" height="57" alt="Logo"></a></div>
        <div class="process-wizard">
            <ul id="progressbar">
                <li class="active"><span>Locations</span></li>
                <li><span>Providers</span></li>
                <li><span>Payment</span></li>
                <li><span>Login</span></li>
            </ul>
        </div>
        <div class="log-frame" layout-align="start end" flex>
    	<md-switch class="md-primary" md-no-ink ng-model="hasInk" aria-label="No Ink Effects" ng-click="text = (text == 'logoff' || text == undefined ? 'logon' : 'logoff')">
            <div ng-if="(text == 'logoff' || text == undefined)">
                <a href="javascript:void(0)" class="logoff">{{text || 'logoff'}}</a>
            </div>    
            <div ng-if="text == 'logon'">
                <a href="javascript:void(0)" class="logon">{{text || 'logoff'}}</a>
            </div>
        </md-switch>
    </div>
    </header>
    <div class="width650">
        <div class="grid100 change-password" ng-controller="formController">
            <fieldset>
                <h2>How Would you like to Login</h2>
                <md-input-container class="md-block">
                    <label>Login ID</label>
                    <input name="login" type="text" ng-model="login.userName">
                </md-input-container>
                <md-input-container class="md-block">
                    <label>Password</label>
                    <input name="password" type="password" ng-model="password.password">
                </md-input-container>
                <md-input-container class="md-block">
                    <label>Confirm Password</label>
                    <input name="password" type="password" ng-model="confirm.password">
                </md-input-container>
                
                <h3>Security Code Setup</h3>
                <md-input-container class="md-block">
                    <label>4 Digits Security Code</label>
                    <input name="security" type="text" ng-model="security.password">
                </md-input-container>
                <md-input-container class="md-block">
                    <label>Confirm Security Code</label>
                    <input name="code" type="text" ng-model="code.password">
                </md-input-container>
                <md-button class="md-warn md-raised save-button next">Let's go set up your account</md-button>
                
            </fieldset>
           


    <fieldset>
      <form  id="formProvider1" name="formProvider">
        <div>
          <div ng-repeat="row in rows track by row.id">

             <h2>Tell About your practice</h2>
                <h3>Primary Location: 1501 Superior Ave, Newport Beach, CA, 92663</h3>
                <div layout-gt-sm="row" class="clearboth">
                    <md-input-container class="md-block">
                        <label>First Name</label>
                        <input name="first_name[]" id="first_name[]" type="text" ng-model="formProvider.first.row.id">
                        <div id="firstname{{ $index }}"></div>
                     </md-input-container>
                    <md-input-container class="md-block">
                        <label>Last Name</label>
                        <input name="last_name[]" id="last_name[]" type="text" ng-model="formProvider.last.row.id">
                        <div id="lastname{{ $index }}"></div>
                    </md-input-container>
                    <div layout="row" layout-align="center start" class="md-block padding20">
                           <md-radio-group layout="row">
                            <input type="radio" value="1" name="gender[]" id="gender[]" class="md-primary">Male
                            <input type="radio" value="0" name="gender[]" id="gender[]" class="md-primary">Female
                        </md-radio-group>
                    </div>
                </div>    
                <md-input-container class="md-block">
                    <label>License Number</label>
                    <input name="license[]" id="license[]" type="text" ng-model="formProvider.license.row.id">
                    <div id="licenseerror{{ $index }}"></div>
                </md-input-container>
                <md-input-container class="md-block" flex-gt-sm>
                    <label>NPI Number</label>
                    <input name="npi[]" name="npi[]" type="text" ng-model="formProvider.npi.row.id">
                     <div id="npierror{{ $index }}"></div>
                </md-input-container>
                
                <md-input-container class="md-block" flex-gt-sm>
                    <md-select placeholder="General Dentist" name="dentist[]" id="dentist[]" ng-model="user">
                        <md-option value="1">Dummy Text1</md-option>
                        <md-option value="2">Dummy Text2</md-option>
                        <md-option value="3">Dummy Text3</md-option>
                    </md-select>
                   <div id="dentisterror{{ $index }}"></div> 
                </md-input-container>
                <div layout="row" layout-align="center center">
                    <md-button class="md-warn md-raised green-button" ng-click=addRow(formProvider)>Add More Providers</md-button>
                </div>
          </div>
        </div>

     <md-button class="md-warn md-raised prev-button previous">Previous</md-button>
    <div id="replace_on_next">
     <md-button  class="md-warn md-raised next-button next" onclick="submitrovider()">Next</md-button>
    </div>
      </form>
