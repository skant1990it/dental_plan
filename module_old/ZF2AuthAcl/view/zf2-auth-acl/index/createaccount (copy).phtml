<script type="text/javascript">  

  function submitrovider(){
            var first_name =document.getElementsByName('first_name[]');
            var last_name  =document.getElementsByName('last_name[]');
            var gender     =document.getElementsByName('gender[]');
            var license    =document.getElementsByName('license[]');
            var npi        =document.getElementsByName('npi[]');
            var dentist    =document.getElementsByName('.dentist[]');
            flag=0;
            for (var i=0; i<first_name.length; i++)
                        {
                            var fnamevalue = first_name[i].value ;
                            if(fnamevalue==''){
                            var firstnameerror =document.getElementById('firstname'+i);
                               firstnameerror.innerHTML= 'Enter First Name'  ;
                               flag=1;
                            }
                            var lnamevalue = last_name[i].value ;
                            if(lnamevalue==''){
                            var lastnameerror =document.getElementById('lastname'+i);
                                lastnameerror.innerHTML= 'Enter Last Name'  ;
                                flag=1;
                            }
                            var licensevalue=license[i].value ;
                             if(licensevalue==''){
                            var licenseerror =document.getElementById('licenseerror'+i);
                                licenseerror.innerHTML= 'Enter License No'  ;
                                flag=1;
                            }
                            var npivalue = npi[i].value;
                             if(npivalue==''){
                            var npierror =document.getElementById('npierror'+i);
                                npierror.innerHTML= 'Enter NPI No'  ;
                                flag=1;
                            }
                            var dentistvalue = parseInt(dentist[i].value) ;
                             if(isNaN(dentistvalue)){
                            var dentisterror =document.getElementById('dentisterror'+i);
                                dentisterror.innerHTML= 'Enter General Dentist.'  ; 
                                flag=1;
                             }

                        }
                        if(flag==0){
                        sendCode(first_name,last_name, gender,license,npi,dentist);
                         
                  }
    }
 
 function sendCode(first_name,last_name, gender,license,npi,dentist) {
    var namevalue = [];
    var lastnamevalue = [];
    var gendervalue = [];
    var licensevalue = [];
    var npivalue = [];
    var dentistvalue = [];
    for (var i=0; i<first_name.length; i++)
   {
        namevalue.push(first_name[i].value) ;
        lastnamevalue.push(last_name[i].value) ;
        gendervalue.push(gender[i].value) ;
        licensevalue.push(license[i].value);
        npivalue.push(npi[i].value) ;
        dentistvalue.push(dentist[i].value) ;
    }
    $.post('/addproviderinfo', {
    fname: namevalue,
    lname: lastnamevalue,
    genderarray: gendervalue,
    npiarray: npivalue,
    dentistarray: dentistvalue,
    licensearray:licensevalue,
    },function(result){
         
     });
   
}





  
var app=angular.module('BlankApp', ['ngMaterial']).controller('formController', function($scope) {   
        $scope.rowCounter = 0;
        $scope.rows = [{
        id: '0'
        }];
        
        $scope.addRow = function() {
        $scope.rowCounter += 1;
        $scope.rowTemplate = {
                id: $scope.rowCounter,
        };
        $scope.rows.push($scope.rowTemplate);
        };
    });

</script>
